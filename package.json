{
  "name": "@hedger/nuxt-sanctum",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "version": "0.1.1",
  "description": "Nuxt module for Laravel Sanctum",
  "repository": "nhedger/nuxt-sanctum",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build prepare && nuxt-module-build",
    "prepack": "pnpm build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare && nuxi prepare playgrounds",
    "release": "pnpm lint && pnpm format && pnpm prepack && changelogen --release && pnpm publish && git push --follow-tags",
    "lint": "biome lint . --apply",
    "format": "biome format . --write && biome check . --apply",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.3",
    "defu": "^6.1.2",
    "ofetch": "^1.3.3",
    "set-cookie-parser-es": "^1.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.2.2",
    "@nuxt/devtools": "^1.0.0",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.3",
    "@nuxt/test-utils": "^3.7.3",
    "@types/node": "^18.17.17",
    "changelogen": "^0.5.5",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.7.3",
    "vitest": "^0.33.0"
  }
}